<div id="<%= dom_id order %>">
  <% if order.user_id %>
    <p style=""><strong><%= get_user(order.user_id).name %></strong></p>
  <% end %>
  <p><%= order.name %></p>
  <div class="mt12 mb12">
    <p><strong>Address</strong></p>
    <p><%= order.address %></p>
    <p><%= order.postcode %> <%= order.city %></p>
  </div>
  <p>
    <strong>Email:</strong>
    <%= order.email %>
  </p>
  <p>
    <strong>Pay type:</strong>
    <%= order.pay_type %>
  </p>
</div>
